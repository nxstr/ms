<UserControl x:Class="ms.LogoutPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ms"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <TextBlock FontSize="20" HorizontalAlignment="Center" Margin="0,122,0,0" TextWrapping="Wrap" Text="Welcome to Meeting Scheduler!" VerticalAlignment="Top" Style="{StaticResource CommonText}"/>
        <Button FontSize="20" Content="Log in" HorizontalAlignment="Center" Margin="0,182,0,0" VerticalAlignment="Top" Click="Button_Click"/>
        <Button FontSize="20" Content="Register" HorizontalAlignment="Center" Margin="0,231,0,0" VerticalAlignment="Top" Click="Button_Click_1"/>

    </Grid>
</UserControl>
